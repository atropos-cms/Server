{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "beyondcode/laravel-self-diagnosis": "^1.3",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "laravel/airlock": "^0.2.0",
        "laravel/framework": "^6.0",
        "laravel/scout": "^7.1",
        "laravel/tinker": "^2.2.0",
        "mll-lab/laravel-graphql-playground": "^2.0.2",
        "nuwave/lighthouse": "dev-master",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "~4.0",
        "spatie/laravel-permission": "^3.0",
        "stancl/tenancy": "^2.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^7.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.3",
        "brainmaestro/composer-git-hooks": "^2.8",
        "facade/ignition": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.4",
        "haydenpierce/class-finder": "^0.4.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "hooks": {
            "pre-commit": [
                "php-cs-fixer fix ."
            ],
            "pre-push": [
                "php-cs-fixer fix --dry-run ."
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@ide-helper"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -N",
            "@php artisan lighthouse:ide-helper"
        ]
    }
}
